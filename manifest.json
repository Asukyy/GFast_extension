{
    "name": "GFast",
    "description": "Send mail faster with GFast",
    "version": "1.0",
    "manifest_version": 3,
    
    "background": {
      "service_worker": "app.js"
    },
    "permissions": ["storage", "activeTab", "scripting"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "/images/16.png"
      }
    },
    "icons": {
        "16": "/images/16.png"
      },
      "options_page": "options.html"
  }